{
    "name": "wefabric/gs1-insbou-order-converter",
    "description": "Package for converting data to and from GS1 Insbou Order XML files.",
    "license" : "MIT",
    "keywords": [
        "wefabric",
        "insbou003",
        "insbou",
        "order",
        "GS1"
    ],
    "autoload": {
        "psr-4": {
            "Wefabric\\GS1InsbouOrderConverter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kevin Nauta",
            "email": "kevin@wefabric.nl"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/var-dumper": "6.*"
    },
    "require": {
        "spatie/data-transfer-object": "^2.8",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "wefabric/strip-empty-elements-from-array": "*",
        "wefabric/simplexml-to-array": "*",
        "wefabric/array-to-simplexml": "*",
        "nesbot/carbon": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wefabric/strip-empty-elements-from-array"
        },
        {
            "type": "vcs",
            "url": "https://github.com/wefabric/simplexml-to-array"
        },
        {
            "type": "vcs",
            "url": "https://github.com/wefabric/array-to-simplexml"
        }
    ]
}
